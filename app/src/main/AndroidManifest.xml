<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.composetrasteo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ComposeTrasteo">

        <activity
            android:name=".ListasElementos"
            android:exported="true"
            android:label="@string/title_activity_listas_elementos"
            android:theme="@style/Theme.ComposeTrasteo.NoActionBar" />
        <activity
            android:name=".ScaffoldComponent"
            android:exported="true"
            android:label="@string/title_activity_scaffold_component"
            android:theme="@style/Theme.ComposeTrasteo.NoActionBar" />
        <activity
            android:name=".SlotsAPI"
            android:exported="true"
            android:label="@string/title_activity_slots_api"
            android:theme="@style/Theme.ComposeTrasteo.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.ComposeTrasteo.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>